{
    "manifest_version": 3,
    "name": "Offline Academy",
    "version": "1.0",
    "description": "Adds download buttons to online locked-ecosystem educational websites and eBook readers",
    "content_scripts": [
        {
            "js": [
                "netacad_content.js"
            ],
            "matches": [
                "https://contenthub.netacad.com/*"
            ]
        },
        {
            "js": [
                "digi4school_content.js"
            ],
            "matches": [
                "https://a.digi4school.at/*"
            ]
        },
        {
            "js": [
                "bedienungsanleitungen_content.js"
            ],
            "matches": [
                "https://www.bedienungsanleitu.ng/*",
                "https://bedienungsanleitu.ng/*"
            ]
        }
    ]
}